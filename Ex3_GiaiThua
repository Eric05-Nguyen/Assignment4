package assignment4;

import java.util.Scanner;

public class B3_ag4 {
    public static int giaithua(int n) {
        int gt = 1;
        for (int i = 2; i <= n; i++) {
            gt *= i;
        }
        return gt;
    }
    public static void main(String[] args) {
        Scanner nhap = new Scanner(System.in);
        System.out.print("n=");
        int n = nhap.nextInt();
        int kq = giaithua(n);
        System.out.println("Factorial = " + kq);
    }
}
