package assignment4;

import java.util.Scanner;

public class B5_ag4 {

    public static void main(String[] args) {
        int dem = 0;
        System.out.print("n=");
        Scanner nhap = new Scanner(System.in);
        int n=nhap.nextInt();
        while (n != 0) {
            dem++;
             n/=10; 
        }
        System.out.println("so chu so la :" + dem);
    }
}
