package assignment4;

import java.util.Scanner;

public class B10_ag4 {
    public static void main(String[] args) {
        Scanner nhap = new Scanner(System.in);
        int n = nhap.nextInt();
        for (int i = 1; i <= n; i++) {
            for (int j = 1; j <= i; j++) {
                System.out.print(j + " ");
            }
            System.out.println();
        }
    }

}
